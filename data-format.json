// Data format from mobile sending to the server
// POST /ibeacon 
{
    "uuid": "FDA50693-A4E2-4FB1-AFCF-C6EB07647825",
    "timestamp": 1231241231312, // seconds time
    "beacons": [{
        "minor": 1,
        "rssi": -73,
        "proximity": 1,
        "accuracy": 0.13,
    }]
}

//POST /pedometers
{
    "timestamp": 1231241231312, // seconds time
    "stepsTaken": 1,
    "distanceTraveled": 1,
    "pace": 1, 
    "cadence": 1
}

//POST /accelerometers
// Once per second

{
    "magneticHeading": 1,
    "readings": [{
        "timestamp": 1231241231312, // seconds time,
        "xAcceleration": 1, 
        "yAcceleration": 1, 
        "zAcceleration": 1
    },
    {
        "timestamp": 1231241231312, // seconds time,
        "xAcceleration": 1, 
        "yAcceleration": 1, 
        "zAcceleration": 1
    }, ]
   
}
